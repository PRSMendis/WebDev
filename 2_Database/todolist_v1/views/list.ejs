<%- include("header") %> 

        <div class = "box" id="heading">
            <h1><%= listTitle %> </h1>
        </div>

        <div class="box">
            <% items.forEach(item => { %>
                
            <div class="item">
                <input type="checkbox">
                <p><%= item %></p>
            </div>
            <% }) %>
            <form class="item" action="/" method="POST">
                <!-- <input type="text"> -->
                <input id="bPoint" type = "text" name = "newItem" placeholder="New Item" autocomplete="off">
                <button type="submit" name="list" value=<%= listTitle %> >+</button>
            </form>

        </div>
        
        
        <script src="" async defer></script>

        <%- include("footer") %> 